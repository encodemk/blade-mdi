{
    "name": "encodemk/blade-mdi",
    "description": "Material Design Icons for Laravel Blade views.",
    "keywords": ["laravel", "php", "blade", "ui", "kit", "icons", "mdi", "material", "design", "icons"],
    "license": "MIT",
    "homepage": "https://github.com/renoki-co/blade-mdi",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "blade-ui-kit/blade-icons": "^0.5.1"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\BladeMdi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\BladeMdi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\BladeMdi\\BladeMdiServiceProvider"
            ]
        }
    }
}
